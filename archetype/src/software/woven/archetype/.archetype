TARGETS:{ime}

+ configuration
    - modelSequence() in "model-sequence.tsv"
    - userTokens() in "user-tokens.triples"

+ languages in "languages"
    - releases(language) in "{language}/releases.json"
    - definition(language) in "{language}/definition.json"

+ models in "models"
    - definition(model) in "{model}/definition.json"
    - workspace(model) in "{model}/workspace"
    - getWorkspaceProperties(model) in "{model}/workspace/properties.json"

+ tmp

+ repository
    + logos
        - getLogo(language) in "{language}.png"
	+ languages